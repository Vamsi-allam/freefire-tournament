# Backend environment variables (copy to .env.local and set real values)
# Database
DB_URL=jdbc:postgresql://<host>:<port>/<db>?sslmode=require
DB_USER=
DB_PASSWORD=

# Mail (GoDaddy or Gmail app password)
EMAIL_USERNAME=
EMAIL_PASSWORD=
EMAIL_FROM_NAME=PrimeArena
EMAIL_REPLY_TO=
EMAIL_LOGO_PATH=

# Business UPI defaults (optional)
BUSINESS_UPI_VPA=
BUSINESS_UPI_NAME=

# Admin emails (comma-separated)
APP_ADMIN_EMAILS=

# JWT
JWT_SECRET=
JWT_EXPIRATION=3600000

# CORS (comma-separated origins). Include both apex and www domains.
# Example: https://primearena.live,https://www.primearena.live,http://localhost:5173
APP_CORS_ALLOWED_ORIGINS=https://primearena.live,https://www.primearena.live
